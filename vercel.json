{
    "version": 2,
    "framework": null,
    "builds": [
      {
        "src": "public/index.php",
        "use": "@vercel/php"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/lte/(.*)", "dest": "public/lte/$1" },
      { "src": "/build/(.*)", "dest": "public/build/$1" },
      { "src": "/(.*)", "dest": "public/index.php" }
    ],
    "buildCommand": "vite build",
    "outputDirectory": "public",
    "public": true,
    "env": {
      "APP_ENV": "production",
      "APP_DEBUG": "true",
